{
    "Version": "2008-10-17",
    "Id": "PolicyForCloudFrontPrivateContent",
    "Statement": [
        {
            "Sid": "1",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::cloudfront:user/CloudFront Origin Access Identity EZTQBUL6N2PCH"
            },
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::production-photolog-photo-cache/*"
        }
    ]
}
,
{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "mobileanalytics:PutEvents",
                "cognito-sync:*",
                "cognito-identity:*"
            ],
            "Principal": "*",
            "Resource": [
                "*"
            ]
        },
        {
            "Effect": "Allow",
            "Principal": "*",
            "Action": [
                "s3:ListBucket",
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject"
            ],
            "Resource": [
				"arn:aws:s3:::production-photolog-image-cache",
				"arn:aws:s3:::production-photolog-image-cache/*"
			]
        }
    ]
}
